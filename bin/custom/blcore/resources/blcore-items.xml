    <relations>
        <relation code="BLProduct2BLSerialProductRelation" autocreate="true" generate="true" localized="false">
            <sourceElement qualifier="blProduct" type="BLProduct" cardinality="one">
                <modifiers read="true" write="true" search="true" optional="false" initial="true"/>
            </sourceElement>
            <targetElement qualifier="serialProducts" type="BLSerialProduct" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true" partof="true"/>
                <custom-properties>
                    <property name="ordering.attribute"><value>"code"</value></property>
                </custom-properties>
            </targetElement>
        </relation>
        <relation code="BlSerial2BlSubpartRelation" autocreate="true" generate="true" localized="false">
            <deployment table="Serial2SubpartRel" typecode="12206"/>
            <sourceElement qualifier="blSerials" type="BlSerialProduct" cardinality="many">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="blSubParts" type="BlSubpart" cardinality="many">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>
    </relations>



    <itemtypes>

