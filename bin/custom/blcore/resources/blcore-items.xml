        <!-- TypeGroups are for structure of this file only -->

        <typegroup name="Apparel">
            <itemtype code="ApparelProduct" extends="Product"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelProduct">
                <description>Base apparel product extension that contains additional attributes.</description>
                <attributes>
                    <attribute qualifier="genders" type="GenderList">
                        <description>List of genders that the ApparelProduct is designed for</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelStyleVariantProduct">
                <description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="style" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color/Pattern of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="swatchColors" type="SwatchColorSet">
                        <description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>

            </itemtype>

            <itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelSizeVariantProduct">
                <description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
                <attributes>
                    <attribute qualifier="size" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Size of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ElectronicsColorVariantProduct">
                <description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
        <typegroup name="borrowLenses">
            <itemtype code="Category" autocreate="false" generate="false">
                <description>Extending from core with additional attributes</description>
                <attributes>
                    <!--  auto ID which is generated by NumberSeries -->
                    <attribute qualifier="categoryId" type="java.lang.String" autocreate="true" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="false" optional="true" unique="true" initial="true"/>
                        <description>Auto generated Category ID</description>
                    </attribute>
                    <attribute qualifier="displayName" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Category Display Name</description>
                    </attribute>
                    <attribute qualifier="titleTag" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Category Title Tag</description>
                    </attribute>
                    <attribute qualifier="active" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>If the category is active</description>
                    </attribute>
                    <attribute qualifier="onCategoryTree" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>The category to be shown on navigation</description>
                    </attribute>
                </attributes>
            </itemtype>
            <itemtype code="BlProduct" extends="Product" autocreate="true" generate="true"
              jaloclass="com.bl.core.jalo.BlProduct">
                <description>Extending to BL product containing the additional attributes.</description>
                <attributes>
                    <!--  auto ID which is generated by NumberSeries -->
                    <attribute qualifier="productId" type="java.lang.String" autocreate="true" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="false" optional="true" unique="true" initial="true"/>
                        <description>Auto generated Product ID</description>
                    </attribute>
                    <attribute qualifier="displayName" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" search="true" write="true" optional="false"/>
                        <description>Product Display Name</description>
                    </attribute>
                    <attribute qualifier="mpn" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Manufactuer's part number</description>
                    </attribute>
                    <attribute qualifier="upc" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="false"/>
                        <description>UPC code for the BL Product</description>
                    </attribute>
                    <attribute qualifier="techEngNotes" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Links to a doc with technical learning about the product</description>
                    </attribute>
                    <attribute qualifier="lrLink" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Link for the rental item on a competitive site</description>
                    </attribute>
                    <attribute qualifier="procurementSource" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Link or number for the item at the source we can purchase replacements</description>
                    </attribute>
                    <attribute qualifier="alternativeProduct" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Message to show for discontinued items</description>
                    </attribute>
                    <attribute qualifier="isVideo" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Product classified as as video for reporting / order reporting</description>
                    </attribute>
                    <attribute qualifier="isAccounting" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="true" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>Used for accounting reporting</description>
                    </attribute>
                    <attribute qualifier="forSale" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="true" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>If the item is available for sale</description>
                    </attribute>
                    <attribute qualifier="forRent" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="true" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>If the item is available for rent</description>
                    </attribute>
                    <attribute qualifier="level1Required" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Level 1 Verification Required</description>
                    </attribute>
                    <attribute qualifier="level2Required" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Level 2 Verification Required</description>
                    </attribute>
                    <attribute qualifier="scheduled" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
                        <description>Determines if availability engine runs on item</description>
                    </attribute>
                    <attribute qualifier="reserveRatio" type="java.lang.Integer">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true" />
                        <description>Sets an override than the default buffer inventory level</description>
                    </attribute>
                    <attribute qualifier="firmwareVersion" type="java.lang.String">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true" />
                        <description>The version the SKU's firmware is currently at</description>
                    </attribute>
                    <attribute qualifier="batteryMaterial" type="BatteryMaterialEnum">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>The type of material the battery is made of</description>
                    </attribute>
                    <attribute qualifier="productType" type="ProductTypeEnum">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="false"/>
                        <description>The type of Product</description>
                    </attribute>
                    <attribute qualifier="mountType" type="MountTypeEnum">
                        <persistence type="property" />
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Mount Type </description>
                    </attribute>
                    <attribute qualifier="shortDescription" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Customer facing notes on the item</description>
                    </attribute>
                    <attribute qualifier="purchaseNotes" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Purchase Notes</description>
                    </attribute>
                    <attribute qualifier="displayNotes" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Customer facing notes on the item</description>
                    </attribute>
                    <attribute qualifier="rentalIncludes" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Customer facing details of what comes with the rental version of the item</description>
                    </attribute>
                    <attribute qualifier="numberingSystem" type="java.lang.String">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Determines how we track inventory</description>
                    </attribute>
                    <!--Shipping Attributes-->
                    <attribute qualifier="weight" type="java.math.BigDecimal">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Used to determine the shipping rate for the item</description>
                    </attribute>
                    <attribute qualifier="height" type="java.lang.Integer">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Used to determine the shipping rate for the item</description>
                    </attribute>
                    <attribute qualifier="width" type="java.lang.Integer">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Used to determine the shipping rate for the item</description>
                    </attribute>
                    <attribute qualifier="length" type="java.lang.Integer">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Used to determine the shipping rate for the item</description>
                    </attribute>
                    <attribute qualifier="procurementCost" type="java.math.BigDecimal">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Starting base price for the sku for used gear sales of all serials</description>
                    </attribute>
                    <attribute qualifier="constrained" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true" optional="false" />
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Determines the product will be constrained or non-constrained</description>
                    </attribute>
                    <attribute qualifier="isNew" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Determines the product is Newly arrived</description>
                    </attribute>
                    <attribute qualifier="discontinued" type="java.lang.Boolean">
                        <persistence type="property" />
                        <modifiers read="true" write="true" search="true"/>
                        <defaultvalue>java.lang.Boolean.FALSE</defaultvalue>
                        <description>Determines the product is Discontinued</description>
                    </attribute>
                    <attribute qualifier="manufacturerAID" type="java.lang.String" redeclare="true">
                        <modifiers read="true" write="true" search="true" optional="false"/>
                        <persistence type="property"/>
                        <custom-properties>
                            <property name="hmcIndexField"><value>"thefield"</value></property>
                        </custom-properties>
                    </attribute>
                    <attribute qualifier="usedDescription" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Customer facing description of the product for used gear listings</description>
                    </attribute>
                    <attribute qualifier="usedIncludes" type="java.lang.String">
                        <persistence type="property">
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Customer facing details of what comes with the for sale version of the item</description>
                    </attribute>
                </attributes>
            </itemtype>
            <itemtype code="BlSerialProduct" extends="BlProduct" autocreate="true" generate="true"
              jaloclass="com.bl.core.jalo.BlSerialProduct">
                <description>Extending BLProduct with additional attributes</description>
                <attributes>
                    <attribute qualifier="serialId" type="java.lang.String">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" unique="true"/>
                        <description>Unique Serial Id for Serial Product</description>
                    </attribute>
                    <attribute qualifier="displayName" type="java.lang.String" redeclare="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true" unique="true"/>
                        <description>Display Name on Serial Product</description>
                    </attribute>
                    <attribute qualifier="barcode" type="java.lang.String">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" unique="true" optional="false"/>
                        <description>Unique Barcode associated to a serial</description>
                    </attribute>
                    <attribute qualifier="warehouseLocation" type="Warehouse">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" unique="true" optional="false"/>
                        <description>Determines the warehouse where the products are kept</description>
                    </attribute>
                    <attribute qualifier="forSaleBasePrice" type="java.math.BigDecimal">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false"/>
                        <description>Starting base price for the sku for used gear sales of all serials</description>
                    </attribute>
                    <attribute qualifier="forSaleDiscount" type="java.lang.Integer">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                        <description>Additional Discount for used gear</description>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="BlSubpart" extends="BlSerialProduct" autocreate="true" generate="true"
              jaloclass="com.bl.core.jalo.BlSubpart">
                <description>Extending BlSerialProduct with additional attributes</description>
                <attributes>
                    <attribute qualifier="serialId" type="java.lang.String" redeclare="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true" unique="true"/>
                        <description>Unique Serial Id for Subpart Product</description>
                    </attribute>
                    <attribute qualifier="barcode" type="java.lang.String" redeclare="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" unique="true"  optional="true"/>
                        <description>Unique Barcode associated to a subpart</description>
                    </attribute>
                    <attribute qualifier="warehouseLocation" type="Warehouse" redeclare="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" unique="true" optional="true"/>
                        <description>Determines the warehouse where the products are kept</description>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
    </itemtypes>
</items>
