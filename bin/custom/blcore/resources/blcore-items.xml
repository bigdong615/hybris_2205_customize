<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
        <collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
        <collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true"
                        type="set"/>
    </collectiontypes>

    <enumtypes>
        <enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
            <value code="BLACK"/>
            <value code="BLUE"/>
            <value code="BROWN"/>
            <value code="GREEN"/>
            <value code="GREY"/>
            <value code="ORANGE"/>
            <value code="PINK"/>
            <value code="PURPLE"/>
            <value code="RED"/>
            <value code="SILVER"/>
            <value code="WHITE"/>
            <value code="YELLOW"/>
        </enumtype>
    </enumtypes>

    <itemtypes>

        <!-- Add your item definitions here -->


        <!-- TypeGroups are for structure of this file only -->

        <typegroup name="Apparel">
            <itemtype code="ApparelProduct" extends="Product"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelProduct">
                <description>Base apparel product extension that contains additional attributes.</description>
                <attributes>
                    <attribute qualifier="genders" type="GenderList">
                        <description>List of genders that the ApparelProduct is designed for</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelStyleVariantProduct">
                <description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="style" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color/Pattern of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="swatchColors" type="SwatchColorSet">
                        <description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>

            </itemtype>

            <itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ApparelSizeVariantProduct">
                <description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
                <attributes>
                    <attribute qualifier="size" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Size of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="com.bl.core.jalo.ElectronicsColorVariantProduct">
                <description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>
        
        <typegroup name="Category">
			<itemtype code="Category" autocreate="false" generate="false">
				<description>Extending from core with additional attributes</description>
				<attributes>
					<!-- auto ID which is generated by NumberSeries -->
					<attribute qualifier="categoryId" type="java.lang.String"
						autocreate="true" generate="true">
						<persistence type="property" />
						<modifiers read="true" write="false" optional="true"
							unique="true" initial="true" />
						<description>Auto generated Category ID</description>
					</attribute>
					<attribute qualifier="displayName" type="java.lang.String">
						<persistence type="property" />
						<modifiers read="true" write="true" optional="false" />
						<description>Category Display Name</description>
					</attribute>
					<attribute qualifier="titleTag" type="java.lang.String">
						<persistence type="property" />
						<modifiers read="true" write="true" optional="true" />
						<description>Category Title Tag</description>
					</attribute>
					<attribute qualifier="active" type="java.lang.Boolean">
						<persistence type="property" />
						<modifiers read="true" write="true" search="true"
							optional="false" />
						<defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
						<description>If the category is active</description>
					</attribute>
					<attribute qualifier="onCategoryTree"
						type="java.lang.Boolean">
						<persistence type="property" />
						<modifiers read="true" write="true" search="true"
							optional="false" />
						<defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
						<description>The category to be shown on navigation</description>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="cms">
			<itemtype code="CustomPromoMessageComponent" autocreate="true"
				generate="true" extends="CMSParagraphComponent">
				<description>Promo Message Component</description>
			</itemtype>
		
			<itemtype code="CustomSiteLogoComponent" autocreate="true"
				generate="true" extends="SimpleBannerComponent">
				<description>SiteLogo Component</description>
			</itemtype>
		
			<itemtype code="CustomBLParagraphComponent"
				autocreate="true" generate="true" extends="CMSParagraphComponent">
				<description>PickUp and Delivery Component</description>
				<attributes>
					<attribute type="localized:java.lang.String" qualifier="mode">
					<description>Mode of Pickup</description>
					<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String"
					qualifier="title">
					<description>Title of pickup or delivery </description>
					<persistence type="property"></persistence>
				</attribute>
				</attributes>				
			</itemtype>
			
			<itemtype code="CustomFeaturedStoriesComponent"
				autocreate="true" generate="true" extends="SimpleBannerComponent">
				<description>Featured Stories</description>
				<attributes>
					<attribute type="localized:java.lang.String"
						qualifier="storyCategory">
						<description>Featured Stories Category </description>
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String"
						qualifier="storyTitle">
						<description>Featured Stories Title </description>
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String"
						qualifier="storyDate">
						<description>Featured Stories Date </description>
						<persistence type="property"></persistence>
					</attribute>
				</attributes>
			</itemtype>
		
		<itemtype code="CustomNavigationPromoComponent" autocreate="true"
				generate="true" extends="SimpleBannerComponent">
				<description>Navigation Promotion Component</description>
			</itemtype>
			
		<itemtype code="RotatingHomePageImagesComponent" generate="true" 
		extends="AbstractBannerComponent" autocreate="true"
		jaloclass="com.bl.core.cms.components.RotatingHomePageImagesComponent">
                <attributes>
                    <attribute type="java.lang.String" qualifier="searchPVRent">
                        <persistence type="property" />
                    </attribute>
                    <attribute type="java.util.Date" qualifier="rentDate">
                        <persistence type="property" />
                    </attribute>
                    <attribute type="java.lang.String" qualifier="rentProData">
						<persistence type="property" />
					</attribute>
					<attribute type="java.lang.String" qualifier="borrowData">
						<persistence type="property" />
					</attribute>
                </attributes>
            </itemtype>

            <itemtype code="RentingGearIsEasyComponent" generate="true" extends="CMSParagraphComponent" autocreate="true">
                <attributes>
                    <attribute type="java.lang.String" qualifier="rentingTitle">
                        <persistence type="property" />
                    </attribute>
                    <attribute type="java.lang.Integer" qualifier="count">
						<persistence type="property" />
					</attribute>
					<attribute type="java.lang.String" qualifier="rentingData1">
						<persistence type="property" />
					</attribute>
					<attribute type="java.lang.String" qualifier="rentingData2">
						<persistence type="property" />
					</attribute>
                </attributes>
            </itemtype>

            <itemtype code="HomePageCategoriesCarouselComponent" autocreate="true" 
            generate="true" extends="AbstractBannerComponent"
			jaloclass="com.bl.core.cms.components.HomePageCategoriesCarouselComponent">
                <attributes>
                    <attribute type="java.lang.String" qualifier="imagetitle">
                        <persistence type="property" />
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="CustomBrandsComponent" autocreate="true" 
            generate="true" extends="AbstractBannerComponent"
            jaloclass="com.bl.core.cms.components.CustomBrandsComponent"/>
            
            <itemtype code="CustomBorrowFromUsComponent" autocreate="true" generate="true" extends="AbstractBannerComponent"
			jaloclass="com.bl.core.jalo.CustomBorrowFromUsComponent"/>
            
            <itemtype code="CustomFooterSubscriptionComponent" autocreate="true" generate="true" extends="SimpleCMSComponent"
            jaloclass="com.bl.core.cms.components.CustomFooterSubscriptionComponent">
				<description>FooterSubscription Component</description>
				<attributes>
					<attribute type="localized:java.lang.String" qualifier="placeHolder">
						<description> Place to add the data for subscription</description>
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="footerAddress">
						<description> Footer Address</description>
						<persistence type="property"></persistence>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="CustomFooterCopyRightComponent" autocreate="true"
			generate="true" extends="CMSParagraphComponent"
			jaloclass="com.bl.core.cms.components.CustomFooterCopyRightComponent">
			<description>Footer details Component</description>
			<attributes>
				<attribute type="localized:java.lang.String" qualifier="footerCopyRight">
				<description>Footer CopyRight</description>
				<persistence type="property"></persistence>
				</attribute>
				<attribute type="localized:java.lang.String" qualifier="footerLinks">
				<description>Footer Links</description>
				<persistence type="property"></persistence>
				</attribute>
			</attributes>
			</itemtype>
			
			<itemtype code="CustomWhyBorrowLensesBannerComponent"
			autocreate="true" generate="true" extends="RotatingImagesComponent">
				<description>WhyBorrow Lenses</description>
				<attributes>
					<attribute type="localized:java.lang.String" qualifier="title">
						<description>WhyBorrow Lenses section title </description>
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="description">
						<description>WhyBorrow Lenses description </description>
						<persistence type="property"></persistence>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="buttonText">
						<description>WhyBorrow Lenses Title </description>
						<persistence type="property"></persistence>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="PickUpOrDeliveryNavigationComponent" autocreate="true"
				generate="true" extends="NavigationComponent"
				jaloclass="de.hybris.platform.acceleratorcms.jalo.components.PickUpOrDeliveryNavigationComponent">
				<description>CMS component for PickUp Or Delivery navigation in the accelerator.</description>
				<attributes>
					<attribute type="localized:java.lang.String"
					qualifier="title">
					<description>Title of pickup or delivery </description>
					<persistence type="property"></persistence>
				</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="SupportNavigationComponent" autocreate="true"
				generate="true" extends="NavigationComponent"
				jaloclass="de.hybris.platform.acceleratorcms.jalo.components.SupportNavigationComponent">
				<description>CMS component for Support navigation in the accelerator.</description>
				<attributes>
					<attribute type="localized:java.lang.String" qualifier="title">
					<description>Title of Support </description>
					<persistence type="property"></persistence>
				</attribute>
				</attributes>
			</itemtype>
		</typegroup>

    </itemtypes>
</items>
