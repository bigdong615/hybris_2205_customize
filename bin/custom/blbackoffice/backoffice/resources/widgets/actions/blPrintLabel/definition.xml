<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<action-definition id="hybris.bin.custom.blbackoffice.blcustom.printshippinglabelaction"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">

    <name>clearpackage.action</name>
    <description>Print Shipping Label Action</description>
    <author>hybris</author>
    <version>0.1</version>

    <actionClassName>com.bl.backoffice.actions.PrintLabelAction</actionClassName>
    <inputType>de.hybris.platform.ordersplitting.model.ConsignmentModel</inputType>
    <outputType>de.hybris.platform.ordersplitting.model.ConsignmentModel</outputType>

    <iconUri>icons/icon_action_print_label_default.png</iconUri>
    <iconDisabledUri>icons/icon_action_print_label_disabled.png</iconDisabledUri>
    <iconHoverUri>icons/icon_action_print_label_hover.png</iconHoverUri>

    <settings>
        <setting key="viewMode" default-value="iconAndText"/>
    </settings>
    
    <!-- Sockets definition-->
    <sockets>
        <output id="blPrintLabelContext"/>
    </sockets>
</action-definition>