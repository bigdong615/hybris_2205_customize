		<property name="tagline" type="java.lang.Boolean"/>
	</bean>

	<bean class="de.hybris.platform.braintreehac.data.SampleMonitorData">
		<property name="results" type="java.util.List&lt;de.hybris.platform.braintreehac.data.SampleMonitorResultData>"/>
