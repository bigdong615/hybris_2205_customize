<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.bl.facades.product.data.GenderData>"/>
	</bean>

	<bean class="com.bl.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class="com.bl.facades.giftcard.data.BLGiftCardData">
		<property name="code" type="String"/>
		<property name="amount" type="java.lang.Double"/>
		<property name="redeemamount" type="java.lang.Double"/>
		<property name="balanceamount" type="java.lang.Double"/>
		<property name="reedemDate" type="java.util.Date" />
		<property name="cartId" type="String" />
		<property name="url" type="String" />
		<property name="currency" type="de.hybris.platform.commercefacades.storesession.data.CurrencyData" />
		<property name="amountc" type="de.hybris.platform.commercefacades.product.data.PriceData" />
		<property name="balanceamountc" type="de.hybris.platform.commercefacades.product.data.PriceData" />
		<property name="indicator" type="String" />
	</bean>

	<bean class="de.hybris.platform.commercefacades.order.data.AbstractOrderData">
		<property name="giftCode" type="String"/>
		<property name="grandTotal" type="PriceData"/>
		<property name="giftCardDiscount" type="PriceData"/>
		<property name="giftCardData" type="java.util.List&lt;com.bl.facades.giftcard.data.BLGiftCardData>"/>
	</bean>

</beans>
