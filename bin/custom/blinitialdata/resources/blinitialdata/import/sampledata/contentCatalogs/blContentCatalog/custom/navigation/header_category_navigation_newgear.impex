                               ;                         ; AllNewGearLink           ; AllNewGear Link      ; "/buy/category/newgear" ; newgear                          ;                                    ; All New Gear        ;

INSERT_UPDATE CMSInverseRestriction;$contentCV[unique=true];name;originalRestriction(&uid);uid[unique=true];
